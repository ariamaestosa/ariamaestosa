// Xcode build configuration

HEADER_SEARCH_PATHS = ../libjdkmidi/include $(SRCROOT)/../ $(SRCROOT)/../Src
OTHER_CFLAGS = -D_MAC_QUICKTIME_COREAUDIO $(WXCONFIG_BUILDFLAGS) $(DEBUG_FLAGS) -DRENDERER_OPENGL
