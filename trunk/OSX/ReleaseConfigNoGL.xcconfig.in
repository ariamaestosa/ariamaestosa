// Xcode build configuration

HEADER_SEARCH_PATHS = ../libjdkmidi/include $(SRCROOT)/../ $(SRCROOT)/../Src
OTHER_CFLAGS = -D_MAC_QUICKTIME_COREAUDIO $(WXCONFIG_BUILDFLAGS) $(DEBUG_FLAGS) -DRENDERER_WXWIDGETS
GCC_WARN_HIDDEN_VIRTUAL_FUNCTIONS = YES
GCC_WARN_CHECK_SWITCH_STATEMENTS = YES
GCC_WARN_SHADOW = YES
GCC_WARN_NON_VIRTUAL_DESTRUCTOR = YES

OTHER_LDFLAGS = $(WXCONFIG_LINKFLAGS) /Developer/libs/libjdkmidi/libjdkmidi.a
LIBRARY_SEARCH_PATHS = /usr/local/lib /usr/lib
